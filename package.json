{
  "name": "cordova-hot-code-push-cli",
  "version": "1.2.0",
  "description": "Cordova Hot Code Push CLI",
  "preferGlobal": "true",
  "bin": {
    "cordova-hcp": "bin/cordova-hcp"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nordnet/cordova-hot-code-push-cli.git"
  },
  "keywords": [
    "cordova",
    "nordnet",
    "mobile",
    "cordova-hot-code-push-plugin",
    "ecosystem:cordova",
    "cordova-android",
    "cordova-ios"
  ],
  "scripts": {
    "pretranspile": "rimraf dist",
    "transpile": "babel src --out-dir dist --source-maps",
    "dev": "npm run transpile -- --watch",
    "eslint": "eslint --ext=.js -c .eslintrc src",
    "test": "mocha --require @babel/register"
  },
  "author": "Charles Quarra",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.14.0",
    "async": "^2.3.0",
    "compression": "^1.5.1",
    "core-js": "^3.13.1",
    "express": "^4.13.1",
    "fs-extra": "^0.22.1",
    "hidefile": "^1.1.0",
    "lodash": "^4.17.21",
    "minimatch": "^3.0.4",
    "prompt": "^1.1.0",
    "q": "^1.4.1",
    "readdirp": "^2.0.0",
    "recursive-readdir": "^2.0.0",
    "socket.io": "^4.1.2",
    "watch": "^0.13.0",
    "yargs": "^16.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-runtime": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@babel/register": "^7.13.16",
    "chai": "^4.3.0",
    "chai-spies": "^1.0.0",
    "eslint": "^7.28.0",
    "eslint-config-airbnb": "0.0.7",
    "eslint-plugin-react": "^3.1.0",
    "mocha": "^8.4.0",
    "rimraf": "^3.0.2",
    "sinon": "^9.2.4",
    "sinon-chai": "^3.7.0"
  },
  "mocha": {
    "recursive": true
  }
}
